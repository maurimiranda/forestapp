<%= bootstrap_form_for titular, url: titulares_path, method: "get", layout: :horizontal, label_col: "col-sm-5", control_col: "col-sm-7" do |f| %>
  <%= f.text_field :nombre, label: "Nombre" %>
  <%= f.text_field :dni, label: "DNI" %>
  <%= f.text_field :cuit, label: "CUIT" %>
  <hr/>
  <%= f.submit "Buscar", class: "btn btn-primary pull-right", id: "search" %>
<% end %>