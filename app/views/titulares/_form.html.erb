<%= bootstrap_form_for @titular, layout: :horizontal, label_col: "col-sm-3", control_col: "col-sm-9" do |f| %>
  <%= render partial: "common/error_messages", locals: { errors: @titular.errors } %>
  <%= f.text_field :nombre, label: "Nombre" %>
  <%= f.text_field :dni, label: "DNI" %>
  <%= f.text_field :cuit, label: "CUIT" %>
  <hr/>
  <%= link_to "Cancelar", :back, class: "btn btn-default" %>
  <%= f.submit "Guardar", class: "btn btn-primary pull-right", id: "save-titular" %>
<% end %>
