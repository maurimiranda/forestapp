<% content_for :head do %>
  <%= javascript_include_tag "http://maps.google.com/maps/api/js?v=3&sensor=false" %>
  <%= javascript_include_tag "http://cdn.leafletjs.com/leaflet-0.7.3/leaflet.js" %>
  <%= javascript_include_tag 'Google' %>
  <%= javascript_include_tag 'leaflet.label' %>
  <%= javascript_include_tag 'actividades' %>
  <%= stylesheet_link_tag "http://cdn.leafletjs.com/leaflet-0.7.3/leaflet.css" %>
  <%= stylesheet_link_tag 'actividades' %>
<% end %>

<div class="row">
  <div class="col-md-12">
    <div id="map" class="map"></div>
  </div>
</div>

<%= javascript_tag do %>
  window.plantaciones = <%= raw @geojson %>
<% end %>